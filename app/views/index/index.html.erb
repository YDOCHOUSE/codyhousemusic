	<title>Cody House</title>
</head>
<body>
<main id="cd-main-content">
	<section id="cd-intro">
		<h1></h1>
		<%= render "icons" %>
		<header class="cd-header">
			<a class="cd-menu-trigger" href="#main-nav">Menu<span></span></a>
		    <li class="social">
		      <svg class="facebook-icon">
                <use xlink:href="#facebook-icon"/>
              </svg>
            </li>
		    <li class="social">
		      <svg class="tumblr-icon">
                <use xlink:href="#tumblr"/>
              </svg>	
		    </li>
		    <li class="social">
		      <svg class="instagram-icon">
                <use xlink:href="#instagram"/>
              </svg>	
		    </li>
		    <li class="social">
		      <a href="https://www.twitter.com/@Chouuse">
		      <svg class="twitter-icon">
                <use xlink:href="#twitter"/>
              </svg>
              </a>	
            </li>
            <li class="social">
              <svg class="soundcloud-icon">
                <use xlink:href="#soundcloud-icon"/>
              </svg>
		    </li>
		</header>
		<div class="cd-blurred-bg"></div>
	</section> <!-- cd-intro -->
</main>

<div class="cd-shadow-layer"></div>

<nav id="main-nav">
	<ul>
		<li><a href="#0"><span>Music</span></a></li>
